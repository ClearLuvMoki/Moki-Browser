{
  "name": "moki-browser",
  "version": "0.0.1",
  "description": "Moki Electron Browser",
  "main": "src/main/index.ts",
  "scripts": {
    "start:main": "cross-env NODE_ENV=development PORT=7777 electronmon -r ts-node/register/transpile-only .",
    "start:preload": "cross-env NODE_ENV=development webpack --config ./config/mainBuild/preload.dev.js",
    "start:renderer": "cross-env NODE_ENV=development PORT=7777  webpack server --config ./config/rendererBuild/webpack.dev.js",
    "dev": "npm run start:renderer "
  },
  "author": "ClearLuvMoki",
  "license": "ISC",
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "antd": "^5.2.1",
    "child_process": "^1.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "electron-log": "5.0.0-beta.16",
    "electron-squirrel-startup": "^1.0.0",
    "electronmon": "^2.0.2",
    "eslint": "8.22.0",
    "eslint-plugin-react": "^7.31.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "mobx": "^6.8.0",
    "mobx-react": "^7.6.0",
    "postcss-loader": "^7.0.2",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.6",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-obfuscator": "^3.5.1"
  },
  "dependencies": {
    "dexie": "^3.2.3",
    "electron": "23.1.0"
  },
  "electronmon": {
    "patterns": [
      "!./src/renderer/**/*",
      "!node_modules"
    ],
    "logLevel": "quiet"
  }
}
